    "prettier": "^2.8.2",
    "pug": "^3.0.2",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.2",
    "typescript": "^4.9.4"
  }
}
